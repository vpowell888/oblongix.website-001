{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "enabled": true
      },
      "google": {
        "enabled": true
      },
      "login": {
        "customOpenIdConnectProviders": [
          {
            "name": "emailauth",
            "authority": "https://<your-custom-domain>.auth0.com/",
            "clientIdSettingName": "AUTH_CLIENT_ID"
          }
        ]
      }
    }
  },
  "routes": [
    {
      "route": "/admin/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/.auth/login/*",
      "statusCode": 200
    }
  ]
}
